  "definitions": {
    "tradingmap": {
      "type": "object",
      "required": ["name", "exchange", "basecurrency", "altcurrency"],
      "properties": {
        "name": {
          "type": "string",
          "default": "Trading Map Template"
        },
        "desc": {
          "type": "string",
          "default": "Description of Trading Map Template"
        },
        "exchange": {
          "type": "string",
          "default": "binance"
        },
        "basecurrency": {
          "type": "string",
          "default": "USDT"
        },
        "altcurrency": {
          "type": "string",
          "default": "BNB"
        },
        "cycleduration": {
          "type": "string",
          "default": "1m"
        }
      }
    },
    "label": {
      "type": "object",
      "required": ["name", "colour"],
      "properties": {
        "name": {
          "type": "string"
        },
        "desc": {
          "type": "string"
        },
        "colour": {
          "type": "string"
        }
      }
    },
    "seed": {
      "type": "object",
      "required": ["data"],
      "properties": {
        "data": {
          "type": "object"
        }
      }
    },
    "classifier": {
      "type": "object",
      "required": ["name", "colour", "type", "data", "confidencevalues"],
      "properties": {
        "name": {
          "type": "string"
        },
        "desc": {
          "type": "string"
        },
        "colour": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "data": {
          "type": "object"
        },
        "confidencevalues": {
          "type": "object"
        }
      }
    },
    "phdselement": {
      "type": "object",
      "required": [
        "exchange",
        "basecurrency",
        "altcurrency",
        "openTime",
        "open",
        "high",
        "low",
        "close",
        "volume",
        "closeTime",
        "quoteVolume",
        "trades",
        "baseAssetVolume",
        "quoteAssetVolume"
      ],
      "properties": {
        "exchange": {
          "type": "string"
        },
        "basecurrency": {
          "type": "string"
        },
        "altcurrency": {
          "type": "string"
        },
        "openTime": {
